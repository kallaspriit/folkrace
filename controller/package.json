{
  "name": "dashboard",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start":
      "concurrently --names \" TSC,SCSS,SYNC\" --prefix-colors \"magenta,yellow,cyan\" \"tsc --watch\" \"yarn build:sass --watch\" \"yarn build:sync --watch\"",
    "build": "yarn tidy:build && yarn build:ts && yarn build:sass && yarn build:sync",
    "build:ts": "tsc",
    "build:sass": "ts-node scripts/sass.ts",
    "build:sync": "ts-node scripts/sync.ts",
    "production": "node build/server/index",
    "lint": "tslint --project tsconfig.json --format codeFrame",
    "lint-fix": "yarn lint --fix",
    "prettier": "prettier --write src/**/*.ts server/**/*.ts",
    "tidy": "yarn tidy:build && yarn tidy:coverage",
    "tidy:build": "rimraf build",
    "tidy:coverage": "rimraf coverage",
    "test": "jest",
    "test-watch": "jest --watch --collectCoverage",
    "coverage": "yarn tidy:coverage && jest --collectCoverage",
    "audit": "yarn prettier && yarn build && yarn lint && yarn coverage"
  },
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/chokidar": "^1.7.5",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.2",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.108",
    "@types/node": "^10.1.0",
    "@types/node-sass": "^3.10.32",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.3",
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "fs-extra": "^6.0.1",
    "jest": "^22.4.3",
    "lodash": "^4.17.10",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-config-strict": "^1.3.2",
    "typescript": "^2.8.3"
  }
}
