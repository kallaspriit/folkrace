{
  "name": "dashboard",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "yarn tidy:build && yarn tidy:app && concurrently --names \"WEBP,SYNC,SERV\" --prefix-colors \"magenta,cyan,yellow\" \"yarn build:webpack --watch\" \"yarn build:sync --watch\" \"yarn server\"",
    "build": "yarn tidy:build && yarn tidy:app && yarn build:webpack && yarn build:sync",
    "build:webpack": "webpack",
    "build:sync": "ts-node scripts/sync.ts",
    "server": "ts-node server/index.ts",
    "lint": "tslint --project tsconfig.json --format codeFrame",
    "lint-fix": "yarn lint --fix",
    "prettier": "prettier --write src/**/*.ts server/**/*.ts",
    "tidy": "yarn tidy:build && yarn tidy:app && yarn tidy:coverage",
    "tidy:build": "rimraf build/**/*",
    "tidy:app": "rimraf ../app/folkbot/src/main/res/raw/**/*",
    "tidy:coverage": "rimraf coverage/**/*",
    "test": "jest",
    "test-watch": "jest --watch --collectCoverage",
    "coverage": "yarn tidy:coverage && jest --collectCoverage",
    "audit": "yarn prettier && yarn build && yarn lint && yarn coverage"
  },
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/chokidar": "^1.7.5",
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^5.0.2",
    "@types/jest": "^22.2.3",
    "@types/lodash": "^4.14.108",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^10.1.0",
    "@types/node-sass": "^3.10.32",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "@types/webpack": "^4.4.0",
    "awesome-typescript-loader": "^5.0.0",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.3",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "express": "^4.16.3",
    "fs-extra": "^6.0.1",
    "jest": "^22.4.3",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-config-strict": "^1.3.2",
    "typescript": "^2.8.3",
    "webpack": "^4.8.3"
  },
  "devDependencies": {
    "webpack-cli": "^2.1.3"
  }
}
