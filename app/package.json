{
  "name": "folkbot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.3.1",
    "color": "^3.1.3",
    "lodash.throttle": "^4.1.1",
    "pathfinding": "^0.4.18",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.0.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "recoil": "0.2.0",
    "ts-log": "^2.2.3",
    "ts-node": "^9.1.1",
    "use-sound": "^2.0.1",
    "victor": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint-fix": "eslint --fix src/**/*.{ts,tsx}",
    "prettier": "npx prettier --write **/*.ts",
    "prettier-check": "npx prettier --list-different **/*.ts",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "sync": "node -r ./scripts/register scripts/sync",
    "sync-watch": "node -r ./scripts/register scripts/sync --watch",
    "deploy": "npm run build && npm run sync"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.7",
    "@storybook/addon-knobs": "^6.2.7",
    "@storybook/addon-links": "^6.2.7",
    "@storybook/addons": "^6.2.7",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.7",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.2",
    "@types/classnames": "^2.2.11",
    "@types/color": "^3.0.1",
    "@types/jest": "^26.0.22",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^14.14.37",
    "@types/pathfinding": "0.0.4",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router": "^5.1.13",
    "@types/react-router-dom": "^5.1.7",
    "@types/victor": "^1.1.0",
    "@types/webpack-env": "^1.16.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^5.1.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4",
    "typescript-plugin-css-modules": "^3.2.0"
  }
